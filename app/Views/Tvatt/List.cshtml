@model app.Models.Tvatt

  <div class="form-group">
            @Html.LabelFor(model => model.TvattDate, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.TvattDate, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.TvattDate, "incorrect date", new { @class = "text-danger" })
            </div>
        </div>

<!--
<form action="/tvatt/add">
  <input type="date" name="TvattDate">
  <input type="submit" value="submit">
  </form>
  -->
<table>
     <tbody>
@foreach (var tvatt in ( List<app.Models.Tvatt>) ViewData["tvatts"])

{
  <tr>
      <td>@tvatt.TvattDate</td>
      <td>@tvatt.User</td>
  </tr>  
}
</tbody>
</table>

